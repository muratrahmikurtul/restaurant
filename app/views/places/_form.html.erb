<%= form_for @place do |f| %>

    <%= f.label :name%>
    <%= f.text_field :name, placeholder: 'name', class: "form-control input-sm" %><br />
    <%= f.label :adress%><br />
    <%= f.text_area :adress, placeholder: 'adress',rows: 10  %><br />
    <%= f.label :established_at%>
    <%= f.date_field :established_at, placeholder: 'established_at'%><br />
    <%= f.label :phone_number%><br />
    <%= f.text_area :phone_number, placeholder: 'phone_number'  %><br />
    <%= f.label :contact_mail%><br />
    <%= f.text_field :contact_mail, placeholder: 'contact_mail'  %><br />
    <%= f.label :city%><br />
    <%= f.text_field :city, placeholder: 'city'  %><br />
    <%= f.select :category_id, @categories %><br />
    <%= f.collection_check_boxes(:food_ids, @foods, :id, :name) %>



    <%=f.fields_for :social_profile do |sf| %>
        <h3>Social Profile</h3>
        <%= sf.label :facebook%>
        <%= sf.text_field :facebook, placeholder: 'facebook', class: "form-control input-sm" %><br />
        <%= sf.label :twitter%>
        <%= sf.text_field :twitter, placeholder: 'twitter', class: "form-control input-sm" %><br />
        <%= sf.label :instagram%>
        <%= sf.text_field :instagram, placeholder: 'instagram', class: "form-control input-sm" %><br />
        <%= sf.label :foursquare%>
        <%= sf.text_field :foursquare, placeholder: 'foursquare', class: "form-control input-sm" %><br />
    <%end%>

    <%= f.submit 'Gonder'%>

<%end%>
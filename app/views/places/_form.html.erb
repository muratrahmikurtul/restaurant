<%= form_for @place do |f| %>

    <%= f.label :name%>
    <%= f.text_field :name, placeholder: 'name', class: "form-control input-sm" %><br />
    <%= f.label :adress%><br />
    <%= f.text_area :adress, placeholder: 'adress',rows: 10  %><br />
    <%= f.label :established_at%>
    <%= f.date_field :established_at, placeholder: 'established_at'%><br />
    <%= f.label :phone_number%><br />
    <%= f.text_area :phone_number, placeholder: 'phone_number'  %><br />
    <%= f.label :contact_mail%><br />
    <%= f.text_field :contact_mail, placeholder: 'contact_mail'  %><br />
    <%= f.label :city%><br />
    <%= f.text_field :city, placeholder: 'city'  %><br />
    <%= f.select :category_id, @categories %><br />
    <%= f.collection_check_boxes(:food_ids, @foods, :id, :name) %>
    <%= f.submit 'Gonder'%>


<%end%>